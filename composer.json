{
	"name": "icanboogie/icanboogie",
	"type": "library",
	"description": "High-performance object-oriented micro-framework for PHP5.3+",
	"keywords": ["framework"],
	"homepage": "http://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/ICanBoogie/issues",
		"source": "https://github.com/ICanBoogie/ICanBoogie"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.0",
		"icanboogie/common": "*",
		"icanboogie/inflector": "*",
		"icanboogie/datetime": "*",
		"icanboogie/prototype": "2.x",
		"icanboogie/activerecord": "2.x",
		"icanboogie/event": "*",
		"icanboogie/http": "2.x",
		"icanboogie/routing": "2.x",
		"icanboogie/operation": "2.x",
		"icanboogie/i18n": "*",
		"icanboogie/errors": "*",
		"icanboogie/module": "2.x"
	},
	"autoload": {
		"classmap": [ "lib/" ],
		"files": [ "bootstrap.php", "lib/helpers.php" ]
	}
}