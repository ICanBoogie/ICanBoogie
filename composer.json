{
	"name": "icanboogie/icanboogie",
	"type": "library",
	"description": "High-performance object-oriented micro-framework for PHP5.3+",
	"keywords": ["framework"],
	"homepage": "http://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/ICanBoogie/issues",
		"source": "https://github.com/ICanBoogie/ICanBoogie"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.0",
		"icanboogie/common": "dev-master as 1.0.4",
		"icanboogie/time": "*",
		"icanboogie/prototype": "1.0.*",
		"icanboogie/activerecord": "1.0.*",
		"icanboogie/event": "1.0.*",
		"icanboogie/http": "1.0.*",
		"icanboogie/routing": "*",
		"icanboogie/operation": "*",
		"icanboogie/i18n": "*",
		"icanboogie/errors": "*"
	},
	"autoload": {
		"classmap": [ "lib/" ],
		"files": [ "bootstrap.php", "lib/helpers.php" ]
	}
}